<h1><%= @member.first_name %>  <%= @member.last_name%></h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Premium</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <tr>
    <td>
      <%= @member.first_name %>
    </td>
    <td>
      <%= @member.last_name %>
    </td>
    <td>
      <% if @member.premium  == "t" %>
      True
      <%else%>
      False
      <% end %>
    </td>
    <td>
      <% if @member.active  == "t" %>
      True
      <%else%>
      False
      <% end %>
    </td>
    <td><form  class ="show_member_form" action="/members/<%= @member.id %>/bookings">
      <input class ="button1" type="submit" value="View Sessions"/>
    </form></td>
    <td>  <form class ="show_member_form" action="/members/<%= @member.id %>/edit">
      <input class ="button1" type="submit" value="Edit Member"/>
    </form></td>
    <td>  <form class ="show_member_form" method="post" action="/members/<%= @member.id %>/delete">
      <input class ="button2" type="submit" value="Delete Member"/>
    </form></td>
  </tr>
</table>

</br>

<form  class ="show_member_form" action="/sessions">
  <input class ="button1" type="submit" value="Book Class"/>
</form>
