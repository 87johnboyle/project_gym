<h1>New Member</h1>

<form method = "POST" action ="/members">
  <div class ="form_space">
    <label for ="first_name">First Name:</label>
    <input class="new_form" id ="first_name" type="text" name = "first_name" />

  </div>
  <div class ="form_space">
    <label for ="last_name">Last Name:</label>
    <input class="new_form" id = "last_name" type="text" name ="last_name"  />

  </div>
  <div class ="form_space">
    <label for ="premium">Select a Membership</label><br />
    <select id="premium" name="premium">
      <option value=True>Premium Membership</option>
      <option value=False>Basic Membership</option>
    </select>

  </div>
  <input class="new" type="submit" value"Submit New Member" />


</form>
