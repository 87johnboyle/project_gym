<h1>New Member</h1>

<form method = "POST" action ="/members">

  <div class ="form_space">
    <label for ="first-name-field">First Name</label>
    <input class="new_form" type ="text" name ="first_name" id="first-name-field" value = "" />
    <br />
  </div>

</br>

  <div class ="form_space">
    <label for="last-name-field">Last Name</label>
    <input class="new_form" type="text" name="last_name" id="last-name-field" value="" />
    <br />
  </div>

</br>

  <div class ="form_space">
    <label for="premium"> Select Membership</label>
    <select name="premium" id="premium">
      <option value = TRUE > Premium</option>
      <option value = FALSE> Basic </option>
    </select>

  </div>

</br>

  <div class ="form_space">
    <label for="active"> Activity</label>
    <select name="active" id="active">
      <option value = TRUE > Active</option>
      <option value = FALSE > Inactive</option>
    </select>

  </div>

</br>

  <input class="button1" type="submit" value"Submit New Member" />


</form>
