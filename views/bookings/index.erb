<h1>All Bookings</h1>

<div class = "table-container">
  <table>
    <tr>
      <th>Member</th>
      <th>Session</th>
      <th></th>
    </tr>
    <% for booking in @bookings %>
    <tr>
      <td><%= booking.member.first_name %></td>
      <td><%= booking.session.name %></td>
      <td>
        <form action="/booking/<%= booking.id %>/delete" method="POST">
          <input class = "button2" type="submit" value="Delete Booking" />
        </form>
      </td>
    </tr>

    <% end %>
  </table>
</div>
